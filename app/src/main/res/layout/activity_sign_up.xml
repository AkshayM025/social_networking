<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.socialnetworking.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearSignUp"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_3pxv"
        android:paddingEnd="@dimen/_16pxh"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_3pxv" style="@style/groupStyledeep_purple_A200">
        <ImageButton
            android:background="@drawable/rectangle_bg_deep_purple_a200_radius_tl_15_bl_15_br_15"
            android:id="@+id/btnArrowleft"
            android:layout_height="@dimen/_44pxh"
            android:layout_width="@dimen/_44pxh"
            android:paddingHorizontal="@dimen/_10pxh"
            android:paddingVertical="@dimen/_10pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_arrowleft_white_a700_1" tools:ignore="ContentDescription"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtCreateaNewAccount"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtCreateaNewAccount}"
            style="@style/txtInterbold32_1" tools:text="@string/msg_create_a_new_ac"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtItsfastandeasy"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtItsfastandeasy}"
            style="@style/txtInterregular16_1" tools:text="@string/msg_it_s_fast_and_e"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowframethirty"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <EditText
                android:background="@drawable/rectangle_bg_deep_purple_a100_cc_radius_25"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:hint="@string/lbl_first_name"
                android:id="@+id/etFrameThirty" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={signUpVM.signUpModel.etFrameThirtyValue}"
                android:textColor="@color/white_A700"
                android:textColorHint="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_deep_purple_a100_cc_radius_25"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:hint="@string/lbl_last_name"
                android:id="@+id/etFrameThirtyFour"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={signUpVM.signUpModel.etFrameThirtyFourValue}"
                android:textColor="@color/white_A700"
                android:textColorHint="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
        </LinearLayout>
        <EditText
            android:background="@drawable/rectangle_bg_deep_purple_a100_cc_radius_25"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:hint="@string/lbl_email_id"
            android:id="@+id/etFrameThirtyFive" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingHorizontal="@dimen/_16pxh"
            android:paddingVertical="@dimen/_16pxv"
            android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etFrameThirtyFiveValue}"
            android:textColor="@color/white_A700"
            android:textColorHint="@color/white_A700"
            android:textFontWeight="500"
            android:textSize="@dimen/_14pxh" android:textStyle="bold" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_deep_purple_a100_cc_radius_25"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:hint="@string/lbl_phone_no"
            android:id="@+id/etFrameThirtySix" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingHorizontal="@dimen/_16pxh"
            android:paddingVertical="@dimen/_16pxv"
            android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etFrameThirtySixValue}"
            android:textColor="@color/white_A700"
            android:textColorHint="@color/white_A700"
            android:textFontWeight="500"
            android:textSize="@dimen/_14pxh" android:textStyle="bold" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_deep_purple_a100_cc_radius_25"
            android:drawableEnd="@drawable/img_calendar"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:hint="@string/lbl_date_of_birth"
            android:id="@+id/etFrameThirtySeven"
            android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etFrameThirtySevenValue}"
            android:textColor="@color/white_A700"
            android:textColorHint="@color/white_A700"
            android:textFontWeight="500"
            android:textSize="@dimen/_14pxh" android:textStyle="bold" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end" android:id="@+id/txtGender"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtGender}"
            style="@style/txtIntermedium16" tools:text="@string/lbl_gender"/>
        <RadioGroup android:id="@+id/rgRadiogroupfemale"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <RadioButton android:buttonTint="@color/deep_purple_A100_cc"
                android:drawablePadding="@dimen/_8pxh"
                android:drawableStart="@drawable/img_eye_24x24"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:id="@+id/rbFemale"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_48pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_female"
                android:textColor="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_female"/>
            <RadioButton android:buttonTint="@color/deep_purple_A100_cc"
                android:drawablePadding="@dimen/_8pxh"
                android:drawableStart="@drawable/img_radiobuttonunchecked"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:id="@+id/rbMale"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_48pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_male"
                android:textColor="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_male"/>
        </RadioGroup>
        <TextView android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@{signUpVM.signUpModel.txtDescription}"
            style="@style/txtInterregular14_1" tools:text="@string/msg_by_clicking_reg"/>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_white_a700_radius_25"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:gravity="center" android:id="@+id/btnSignup"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_157pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_sign_up"
            android:textAllCaps="false"
            android:textColor="@color/deep_purple_A200"
            android:textFontWeight="500"
            android:textSize="@dimen/_18pxh" android:textStyle="bold" tools:text="@string/lbl_sign_up"/>
    </LinearLayout>
</layout>
