<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pageViewVM" type="com.socialnetworking.app.modules.pageview.data.viewmodel.PageViewVM"/>
    </data>
    <LinearLayout android:id="@+id/linearPageView"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft_white_a700"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>
            <LinearLayout android:id="@+id/linearGroup30"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageReply"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_reply"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_reply"/>
                <ImageView android:id="@+id/imageGroup9164"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group8916"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group8916"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumngroup2763"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_40pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_16pxh" android:paddingTop="@dimen/_40pxv">
            <FrameLayout android:id="@+id/frameGroup2763"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_deep_purple_a200_radius_14"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:gravity="center" android:id="@+id/btn4Six"
                android:layout_height="@dimen/_28pxv"
                android:layout_width="@dimen/_71pxh"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true" android:text="@string/lbl_4_6"
                android:textAllCaps="false"
                android:textColor="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_4_6"/>
            <TextView android:id="@+id/txtHalifordLuxury"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{pageViewVM.pageViewModel.txtHalifordLuxury}"
                style="@style/txtInterbold40" tools:text="@string/msg_haliford_luxury"/>
            <TextView android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:text="@{pageViewVM.pageViewModel.txtDescription}"
                style="@style/txtInterregular12" tools:text="@string/msg_halford_hotel_i"/>
            <LinearLayout android:id="@+id/linearRowellipsenine"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseNine"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse9_40x40_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse9_40x40_1"/>
                <ImageView android:id="@+id/imageEllipseTen"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse10_40x40_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse10_40x40_1"/>
                <ImageView android:id="@+id/imageEllipseEleven"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse11_40x40_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse11_40x40_1"/>
                <FrameLayout android:id="@+id/frameGroup2764"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <ImageView android:id="@+id/imageLightbulb"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lightbulb"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtZipcode"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{pageViewVM.pageViewModel.txtZipcode}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_2200"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_18x19"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_18x19"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEightHundred"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{pageViewVM.pageViewModel.txtEightHundred}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_800"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowframethirtysix"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_70_radius_25"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:hint="@string/lbl_write_a_comment"
                android:id="@+id/etFrameThirtySix"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_12pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={pageViewVM.pageViewModel.etFrameThirtySixValue}"
                android:textColor="@color/white_A700"
                android:textColorHint="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_deep_purple_a200_radius_25"
                android:id="@+id/btnSend"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_50pxh"
                android:layout_width="@dimen/_50pxh"
                android:paddingHorizontal="@dimen/_13pxh"
                android:paddingVertical="@dimen/_13pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_send_50x50" tools:ignore="ContentDescription"/>
        </LinearLayout>
    </LinearLayout>
</layout>
