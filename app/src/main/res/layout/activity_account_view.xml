<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountViewVM" type="com.socialnetworking.app.modules.accountview.data.viewmodel.AccountViewVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStack"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearAccountView"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="center_horizontal"
                android:id="@+id/frameStackgroup"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackrectangle3809"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_645pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageRectangle3809"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle3809_645x414}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3809_645x414"/>
                    <androidx.appcompat.widget.Toolbar
                        android:id="@+id/toolbarAppbar"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" app:contentInsetStart="0dp">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_deep_purple_a200"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_deep_purple_a200"/>
                        <ImageView android:id="@+id/imageGroup9164"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group9164"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group9164"/>
                    </androidx.appcompat.widget.Toolbar>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearColumngroup"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle_4">
                    <ImageView android:id="@+id/imageGroup"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_130pxh"
                        android:layout_width="@dimen/_130pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                    <FrameLayout android:id="@+id/frameGroup2668"
                        android:layout_height="wrap_content" android:layout_width="match_parent"/>
                    <LinearLayout
                        android:id="@+id/linearColumnellipseeleven"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_33pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_33pxv" style="@style/groupStyledeep_purple_2">
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearRowellipseeleven"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageEllipseEleven"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_50pxh"
                                android:layout_width="@dimen/_50pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse11_50x50}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse11_50x50"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnrosalia"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtRosalia"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtRosalia}"
                                    style="@style/txtIntersemibold20_2" tools:text="@string/lbl_rosalia"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtRoseTwentyThree"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtRoseTwentyThree}"
                                    style="@style/txtInterregular14_1" tools:text="@string/lbl_rose23"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameGroup2669"
                                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:background="@drawable/rectangle_border_white_a700_radius_16"
                                android:ellipsize="end"
                                android:fontFamily="@font/inter"
                                android:gravity="center"
                                android:id="@+id/btnFollow"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_33pxv"
                                android:layout_width="@dimen/_98pxh"
                                android:paddingBottom="@dimen/_6pxv"
                                android:paddingEnd="@dimen/_24pxh"
                                android:paddingStart="@dimen/_24pxh"
                                android:paddingTop="@dimen/_6pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_follow"
                                android:textAllCaps="false"
                                android:textColor="@color/white_A700"
                                android:textFontWeight="600"
                                android:textSize="@dimen/_16pxh"
                                android:textStyle="bold" tools:text="@string/lbl_follow"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowpost"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearColumnpost"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtPost"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtPost}"
                                    style="@style/txtInterregular20_1" tools:text="@string/lbl_post"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtSeventyFive"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtSeventyFive}"
                                    style="@style/txtIntersemibold20_2" tools:text="@string/lbl_75"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearColumnfollowing"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtFollowing"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtFollowing}"
                                    style="@style/txtInterregular20_1" tools:text="@string/lbl_following"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtZipcode"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtZipcode}"
                                    style="@style/txtIntersemibold20_2" tools:text="@string/lbl_3400"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearColumnfollowers"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtFollowers"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtFollowers}"
                                    style="@style/txtInterregular20_1" tools:text="@string/lbl_followers"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtZipcodeOne"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accountViewVM.accountViewModel.txtZipcodeOne}"
                                    style="@style/txtIntersemibold20_2" tools:text="@string/lbl_6500"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowgoogle"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageButton
                                android:background="@drawable/rectangle_bg_white_a700_radius_25"
                                android:id="@+id/btnGoogle"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_50pxh"
                                android:layout_width="@dimen/_50pxh"
                                android:paddingHorizontal="@dimen/_13pxh"
                                android:paddingVertical="@dimen/_13pxv"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_google_50x50" tools:ignore="ContentDescription"/>
                            <ImageButton
                                android:background="@drawable/rectangle_bg_white_a700_radius_25"
                                android:id="@+id/btnComputer"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_50pxh"
                                android:layout_width="@dimen/_50pxh"
                                android:paddingHorizontal="@dimen/_13pxh"
                                android:paddingVertical="@dimen/_13pxv"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_computer_50x50" tools:ignore="ContentDescription"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/white_A700"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_50pxh"
            app:srcCompat="@drawable/img_call" app:tint="@color/deep_purple_A200"/>
    </FrameLayout>
</layout>
