<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginScreenVM" type="com.socialnetworking.app.modules.loginscreen.data.viewmodel.LoginScreenVM"/>
    </data>
    <LinearLayout android:id="@+id/linearLoginScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_A200">
        <LinearLayout android:id="@+id/linearColumnrectangle3799"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="center_horizontal"
                android:id="@+id/frameStackrectangle3799"
                android:layout_height="@dimen/_243pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle3799"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_158pxh"
                    android:layout_width="@dimen/_122pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_rectangle3799_158x122}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3799_158x122"/>
                <ImageView android:id="@+id/imageRectangle3800"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_158pxh"
                    android:layout_width="@dimen/_122pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_rectangle3800_158x122}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3800_158x122"/>
                <ImageView android:id="@+id/imageRectangle3803"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_67pxh"
                    android:layout_width="@dimen/_122pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_rectangle3803_67x122}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3803_67x122"/>
                <ImageView android:id="@+id/imageRectangle3802"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_158pxh"
                    android:layout_width="@dimen/_122pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_rectangle3802_158x122}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3802_158x122"/>
                <ImageView android:id="@+id/imageRectangle3801"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_158pxh"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_4pxh}"
                    imageRes="@{@drawable/img_rectangle3801_158x8}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3801_158x8"/>
                <ImageView android:id="@+id/imageRectangle3799One"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_158pxh"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_4pxh}"
                    imageRes="@{@drawable/img_rectangle3799_158x8_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3799_158x8_1"/>
                <View
                    android:background="@drawable/rectangle_gradient_s_deep_purple_a200_f2_e_deep_purple_a200_f2_01"
                    android:id="@+id/viewRectangleOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_243pxv" android:layout_width="@dimen/_414pxh"/>
            </FrameLayout>
            <TextView android:gravity="start"
                android:id="@+id/txtFindnewfriend"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_2pxh"
                android:text="@{loginScreenVM.loginScreenModel.txtFindnewfriend}"
                style="@style/txtInterbold32_1" tools:text="@string/msg_find_new_friend"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{loginScreenVM.loginScreenModel.txtDescription}"
                style="@style/txtInterregular16_1" tools:text="@string/msg_with_billions_o"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrloginwith"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{loginScreenVM.loginScreenModel.txtOrloginwith}"
                style="@style/txtInterregular16_1" tools:text="@string/lbl_or_login_with"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowfacebook"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFacebook"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_facebook"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                <ImageView android:id="@+id/imageTwitter"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_twitter"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_twitter"/>
                <ImageView android:id="@+id/imageGoogle"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_google"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_white_a700_radius_25"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:gravity="center" android:id="@+id/btnLogin"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_164pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@string/lbl_log_in"
                android:textAllCaps="false"
                android:textColor="@color/deep_purple_A200"
                android:textFontWeight="500"
                android:textSize="@dimen/_18pxh"
                android:textStyle="bold" tools:text="@string/lbl_log_in"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_border_white_a700_radius_25"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:gravity="center" android:id="@+id/btnSignup"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_157pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_sign_up"
            android:textAllCaps="false"
            android:textColor="@color/white_A700"
            android:textFontWeight="500"
            android:textSize="@dimen/_18pxh" android:textStyle="bold" tools:text="@string/lbl_sign_up"/>
    </LinearLayout>
</layout>
