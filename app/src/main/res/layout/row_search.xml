<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchRowModel" type="com.socialnetworking.app.modules.search.data.model.SearchRowModel"/>
    </data>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearRowellipsefive"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageEllipseFive"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_50pxh"
            android:layout_width="@dimen/_50pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_ellipse5_50x50}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse5_50x50"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnname"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtName"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{searchRowModel.txtName}"
                style="@style/txtIntersemibold20_1" tools:text="@string/lbl_kevin_allsrub"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRelationship"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{searchRowModel.txtRelationship}"
                style="@style/txtInterregular14_2" tools:text="@string/msg_your_e_friends"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameGroup2659"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_border_deep_purple_a200_radius_16"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:gravity="center" android:id="@+id/btnFollow"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_33pxv"
            android:layout_width="@dimen/_76pxh"
            android:paddingBottom="@dimen/_8pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_8pxv" android:singleLine="true"
            android:text="@string/lbl_follow"
            android:textAllCaps="false"
            android:textColor="@color/deep_purple_A200"
            android:textFontWeight="600"
            android:textSize="@dimen/_14pxh" android:textStyle="bold" tools:text="@string/lbl_follow"/>
    </LinearLayout>
</layout>
