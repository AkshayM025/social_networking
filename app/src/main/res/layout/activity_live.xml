<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="liveVM" type="com.socialnetworking.app.modules.live.data.viewmodel.LiveVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearLive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumngroup2717"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <LinearLayout android:id="@+id/linearColumnellipsenine"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_1">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarAppbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <ImageView android:id="@+id/imageEllipseNine"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse9_50x50}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse9_50x50"/>
                    <LinearLayout android:id="@+id/linearGroup22"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{liveVM.liveModel.txtLanguage}"
                            style="@style/txtIntersemibold20_2" tools:text="@string/lbl_lucas_anna"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt3516"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{liveVM.liveModel.txt3516}"
                            style="@style/txtInterregular14_1" tools:text="@string/lbl_35_16"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_24x24"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_24x24"/>
                </androidx.appcompat.widget.Toolbar>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameGroup2717"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:id="@+id/linearColumngroup9142"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_29pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_29pxv" style="@style/groupStyle_2">
                <LinearLayout android:id="@+id/linearRowgroup9142"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnellipsenineone"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <LinearLayout
                            android:id="@+id/linearRowellipsenineone"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/_5pxv"
                            android:paddingEnd="@dimen/_8pxh"
                            android:paddingStart="@dimen/_8pxh"
                            android:paddingTop="@dimen/_5pxv" style="@style/groupStyleblack_900_87cornerRadius">
                            <ImageView
                                android:id="@+id/imageEllipseNineOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_45pxh"
                                android:layout_width="@dimen/_45pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse9_45x45_1}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse9_45x45_1"/>
                            <TextView android:gravity="center_vertical"
                                android:id="@+id/txtErlikheSweetCanOne"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:text="@{liveVM.liveModel.txtErlikheSweetCanOne}"
                                style="@style/txtHeeboregular18" tools:text="@string/msg_erlikhe_sweet_c"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowellipseeight"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_8pxh"
                            android:paddingStart="@dimen/_8pxh"
                            android:paddingTop="@dimen/_7pxv" style="@style/groupStyleblack_900_87cornerRadius">
                            <ImageView
                                android:id="@+id/imageEllipseEight"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_45pxh"
                                android:layout_width="@dimen/_45pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse8_45x45_1}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse8_45x45_1"/>
                            <TextView android:gravity="center_vertical"
                                android:id="@+id/txtDongKhuwanSoBeautiful"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:text="@{liveVM.liveModel.txtDongKhuwanSoBeautiful}"
                                style="@style/txtInterregular18_1" tools:text="@string/msg_dong_khuwan_so"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageGroup9142"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_width="@dimen/_98pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group9142"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group9142"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowframethirtysix"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_70_radius_25"
                        android:ellipsize="end"
                        android:fontFamily="@font/inter"
                        android:hint="@string/lbl_write_a_comment"
                        android:id="@+id/etFrameThirtySix"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingBottom="@dimen/_16pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_16pxv"
                        android:singleLine="true"
                        android:text="@={liveVM.liveModel.etFrameThirtySixValue}"
                        android:textColor="@color/white_A700"
                        android:textColorHint="@color/white_A700"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_deep_purple_a200_radius_25"
                        android:id="@+id/btnSend"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_width="@dimen/_50pxh"
                        android:paddingHorizontal="@dimen/_13pxh"
                        android:paddingVertical="@dimen/_13pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_send_50x50" tools:ignore="ContentDescription"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <View android:background="@color/white_A700_3f"
            android:id="@+id/lineRectangle3780"
            android:layout_height="@dimen/_2pxv" android:layout_width="@dimen/_344pxh"/>
    </LinearLayout>
</layout>
